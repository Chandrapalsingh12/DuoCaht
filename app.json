{
  "expo": {
    "name": "Duo Chat",
    "slug": "DuoChat2",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_scrren.png",
      "resizeMode": "contain",
      "backgroundColor": "#8173D6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.theluci2.duochat",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo1.png",
        "backgroundColor": "#8173D6"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}